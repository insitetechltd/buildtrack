{
  "cli": {
    "version": ">= 16.23.1",
    "appVersionSource": "remote"
  },
  "build": {
    "_development_DISABLED": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "COCOAPODS_DISABLE_STATS": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "credentialsSource": "remote",
        "cocoapods": "1.16.1"
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "COCOAPODS_DISABLE_STATS": "true",
        "EXPO_SKIP_NATIVE_DEPENDENCIES_VERSION_CHECK": "1"
      }
    },
    "simulator": {
      "distribution": "internal",
      "ios": {
        "credentialsSource": "remote",
        "cocoapods": "1.16.1",
        "simulator": true
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "COCOAPODS_DISABLE_STATS": "true",
        "EXPO_SKIP_NATIVE_DEPENDENCIES_VERSION_CHECK": "1"
      }
    },
    "expo-go": {
      "distribution": "internal",
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug",
        "credentialsSource": "local"
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "COCOAPODS_DISABLE_STATS": "true"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "credentialsSource": "remote",
        "cocoapods": "1.16.1"
      },
      "android": {
        "credentialsSource": "remote",
        "gradleCommand": ":app:bundleRelease"
      },
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "COCOAPODS_DISABLE_STATS": "true",
        "EXPO_SKIP_NATIVE_DEPENDENCIES_VERSION_CHECK": "1"
      }
    },
    "production-local": {
      "extends": "production",
      "distribution": "store",
      "ios": {
        "credentialsSource": "remote",
        "cocoapods": "1.16.1",
        "autoIncrement": true
      },
      "android": {
        "credentialsSource": "remote",
        "gradleCommand": ":app:bundleRelease"
      },
      "env": {
        "EXPO_APPLE_ID": "tristan.koo@insiteworks.com",
        "EXPO_APPLE_TEAM_ID": "DSNR656S6Y"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6754898737"
      }
    }
  }
}
